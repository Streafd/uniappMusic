.detail-play {
  width: 580rpx;
  height: 580rpx;
  background-size: cover;
  margin: 214rpx auto 0 auto;
  position: relative;
}
.detail-play .detail-play-img {
  width: 370rpx;
  height: 370rpx;
  border-radius: 50%;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
  -webkit-animation: go 10s linear infinite;
          animation: go 10s linear infinite;
  -webkit-animation-play-state: paused;
          animation-play-state: paused;
}
.detail-play .detail-play-run {
  -webkit-animation-play-state: running;
          animation-play-state: running;
}
@-webkit-keyframes go {
from {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
to {
    -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
}
}
@keyframes go {
from {
    -webkit-transform: rotate(0deg);
            transform: rotate(0deg);
}
to {
    -webkit-transform: rotate(360deg);
            transform: rotate(360deg);
}
}
.detail-play text {
  width: 100rpx;
  height: 100rpx;
  font-size: 100rpx;
  color: #fff;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  margin: auto;
}
.detail-play view {
  width: 230rpx;
  height: 360rpx;
  position: absolute;
  top: -200rpx;
  left: 80rpx;
  right: 0;
  bottom: 0;
  margin: auto;
  background-size: cover;
}
.detail-lyric {
  font-size: 32rpx;
  line-height: 82rpx;
  height: 246rpx;
  text-align: center;
  overflow: hidden;
}
.detail-lyric .detail-lyric-warp {
  transition: .5s;
}
.detail-lyric .detail-lyric-warp .detail-lyric-item {
  line-height: 82rpx;
  color: #6f6e73;
}
.detail-lyric .detail-lyric-warp .detail-lyric-item-active {
  color: #fff;
}
.detail-like {
  margin: 0 30rpx;
}
.detail-like .detail-like-head {
  font-size: 36rpx;
  color: #fff;
  margin: 50rpx 0;
}
.detail-like .detail-like-item {
  display: flex;
  align-items: center;
  margin-bottom: 28rpx;
}
.detail-like .detail-like-item .detail-like-img {
  width: 82rpx;
  height: 82rpx;
  border-radius: 20rpx;
  overflow: hidden;
  margin-right: 20rpx;
}
.detail-like .detail-like-item .detail-like-img image {
  width: 100%;
  height: 100%;
}
.detail-like .detail-like-song {
  flex: 1;
}
.detail-like .detail-like-song .detail-like-title {
  font-size: 28rpx;
  color: #fff;
  margin-bottom: 12rpx;
}
.detail-like .detail-like-song:nth-child(2) {
  font-size: 22rpx;
  color: #fff;
}
.detail-like .detail-like-song:nth-child(2) text {
  font-size: 20rpx;
  color: red;
  margin-right: 10rpx;
}
.detail-like text {
  font-size: 50rpx;
  line-height: 1.5;
}
.detail-comment {
  margin: 0 30rpx;
}
.detail-comment .detail-comment-head {
  font-size: 36rpx;
  color: #fff;
  margin: 50rpx 0;
}
.detail-comment .detail-comment-item {
  margin-bottom: 28rpx;
  display: flex;
}
.detail-comment .detail-comment-item .detail-comment-img {
  width: 64rpx;
  height: 64rpx;
  border-radius: 50%;
  overflow: hidden;
  margin-right: 18rpx;
}
.detail-comment .detail-comment-item .detail-comment-img image {
  width: 100%;
  height: 100%;
}
.detail-comment .detail-comment-item .detail-comment-content {
  flex: 1;
  color: #cacbcf;
}
.detail-comment .detail-comment-item .detail-comment-content .detail-comment-title {
  display: flex;
  justify-content: space-between;
}
.detail-comment .detail-comment-item .detail-comment-content .detail-comment-title .detail-comment-name .detail-comment-nickname {
  font-size: 26rpx;
}
.detail-comment .detail-comment-item .detail-comment-content .detail-comment-title .detail-comment-name .detail-comment-tiem {
  font-size: 20rpx;
}
.detail-comment .detail-comment-item .detail-comment-content .detail-comment-title .detail-comment-like {
  font-size: 28rpx;
}
.detail-comment .detail-comment-item .detail-comment-content .detail-comment-text {
  font-size: 28rpx;
  line-height: 40rpx;
  margin-top: 20rpx;
  color: #fff;
  border-bottom: 1px solid #e0e0e0;
  padding-bottom: 40rpx;
}

